<%-include ("partials/header")%>

<h1>This is the index page</h1>

<% blogs.forEach(function(blog){%>
    <div>
        <h2><%=blog.title%></h2>
        <img src="<%=blog.image%>"
        <br>
        <span><%=blog.body%></span><br>
        <%=blog.created%>
    </div>
<%})%>

<%-include ("partials/footer")%>